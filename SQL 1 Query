SELECT head_of_department FROM departments;
-------------------------------------------------
SELECT * 
FROM degrees
WHERE level = 'magistrale';
------------------------------------------------
SELECT *
FROM students
WHERE name = 'Marco';
----------------------------------------------
SELECT * FROM courses
WHERE cfu >= 12;
----------------------------------------------
SELECT * FROM courses
WHERE cfu > 10
OR cfu < 5;
---------------------------------------------
SELECT * FROM courses
WHERE year <= 1
AND period <= 'I semestre';
--------------------------------------------
SELECT * FROM courses
WHERE website IS NULL;
--------------------------------------------
SELECT * FROM teachers
WHERE phone IS NOT NULL;
